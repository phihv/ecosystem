{
  "connector.class": "io.debezium.connector.mysql.MySqlConnector",
  "name": "debezium-mysql-ecosystem-server",

  "database.user": "kafka",
  "database.server.id": "10201",
  "database.server.name": "ecosystem_server",
  "database.port": "3306",
  "database.hostname": "mysql_db",
  "database.password": "12345",
  "database.include.list": "ecosystem",
  "table.include.list": "ecosystem.users",

  "tasks.max": "1",
  "schema.history.internal.kafka.bootstrap.servers": "kafka-1:9094,kafka-2:9094,kafka-3:9094",


  "key.converter": "io.confluent.connect.avro.AvroConverter",
  "key.converter.schema.registry.url": "http://schema-registry:8081",
  "key.converter.basic.auth.credentials.source": "USER_INFO",
  "key.converter.basic.auth.user.info": "admin:12345",

  "value.converter": "io.confluent.connect.avro.AvroConverter",
  "value.converter.schema.registry.url": "http://schema-registry:8081",
  "value.converter.basic.auth.credentials.source": "USER_INFO",
  "value.converter.basic.auth.user.info": "admin:12345",

  "tombstones.on.delete": "false",
  "topic.prefix": "ecosystem_server",
  "schema.history.internal.kafka.topic": "schema-history.ecosystem_server",


  "snapshot.mode": "initial"
}